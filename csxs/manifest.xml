<?xml version='1.0' encoding='UTF-8'?>
<ExtensionManifest
        ExtensionBundleId="com.atomic.ai-sessions"
        ExtensionBundleVersion="1.0.0"
        Version="7.0"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <ExtensionList>
        <Extension Id="com.atomic.ai-sessions.panel" Version="1.0.0" />
    </ExtensionList>
    <ExecutionEnvironment>
        <HostList>
            <Host Name="ILST" Version="19" />
        </HostList>
        <LocaleList>
            <Locale Code="All" />
        </LocaleList>
        <RequiredRuntimeList>
            <RequiredRuntime Name="CSXS" Version="7.0" />
        </RequiredRuntimeList>
    </ExecutionEnvironment>
    <DispatchInfoList>
        <Extension Id="com.atomic.ai-sessions.panel">
            <DispatchInfo>
                <Resources>
                    <MainPath>./client/index.html</MainPath>
                    <ScriptPath>./host/index.jsx</ScriptPath>
                    <CEFCommandLine/>
                </Resources>
                <Lifecycle>
                    <AutoVisible>true</AutoVisible>
                </Lifecycle>
                <UI>
                    <Type>Panel</Type>
                    <Menu>Ai Sessions</Menu>
                    <Geometry>
                        <Size>
                            <Width>300</Width>
                            <Height>360</Height>
                        </Size>
                        <MaxSize>
                            <Width>300</Width>
                            <Height>360</Height>
                        </MaxSize>
                        <MinSize>
                            <Width>300</Width>
                            <Height>360</Height>
                        </MinSize>
                    </Geometry>
                    <Icons>
                        <Icon Type="Normal">./icon.png</Icon>
                        <Icon Type="RollOver">./icon.png</Icon>
                        <Icon Type="DarkNormal">./icon.png</Icon>
                        <Icon Type="DarkRollOver">./icon.png</Icon>
                    </Icons>
                </UI>
            </DispatchInfo>
        </Extension>
    </DispatchInfoList>
</ExtensionManifest>
